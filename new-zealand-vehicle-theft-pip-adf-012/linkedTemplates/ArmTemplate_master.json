{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "new-zealand-vehicle-theft-pip-adf-012"
		},
		"Lin_ser_for_adls_gen_sink_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Lin_ser_for_adls_gen_sink_data'"
		},
		"AzureKeyVault1_for_on_prem_password_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://on-prem-local-credential.vault.azure.net/"
		},
		"Lin_ser_for_adls_gen_sink_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newzealandstorageacount.dfs.core.windows.net/"
		},
		"Lin_ser_for_on_prem_service_source_data_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "@{linkedService().Sourcepath}"
		},
		"Lin_ser_for_on_prem_service_source_data_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "syamm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "new-zealand-vehicle-theft-pip-adf-012_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Lin_ser_for_adls_gen_sink_data_accountKey": {
						"value": "[parameters('Lin_ser_for_adls_gen_sink_data_accountKey')]"
					},
					"AzureKeyVault1_for_on_prem_password_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_for_on_prem_password_properties_typeProperties_baseUrl')]"
					},
					"Lin_ser_for_adls_gen_sink_data_properties_typeProperties_url": {
						"value": "[parameters('Lin_ser_for_adls_gen_sink_data_properties_typeProperties_url')]"
					},
					"Lin_ser_for_on_prem_service_source_data_properties_typeProperties_host": {
						"value": "[parameters('Lin_ser_for_on_prem_service_source_data_properties_typeProperties_host')]"
					},
					"Lin_ser_for_on_prem_service_source_data_properties_typeProperties_userId": {
						"value": "[parameters('Lin_ser_for_on_prem_service_source_data_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}